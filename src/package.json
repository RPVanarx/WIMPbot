{
    "name": "wimp",
    "version": "1.0.0",
    "description": "telegram bot",
    "main": "index.js",
    "scripts": {
        "start": "node app.js",
        "test-web-api": "jest /web-api --runInBand",
        "test-web-api-watch": "jest --watchAll --runInBand /web-api"
    },
    "keywords": [
        "telegram",
        "bot"
    ],
    "author": "RPVanarx",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^5.13.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-prettier": "^3.0.1",
        "jest": "^24.1.0",
        "prettier": "^1.16.4",
        "supertest": "^3.4.2"
    },
    "dependencies": {
        "@cesium133/forgjs": "1.1.8",
        "axios": "^0.18.0",
        "busboy": "^0.3.0",
        "dotenv": "^6.2.0",
        "koa": "^2.7.0",
        "koa-multer": "^1.0.2",
        "koa-router": "^7.4.0",
        "pg": "^7.6.0",
        "telegraf": "^3.25.0"
    },
    "prettier": {
        "bracketSpacing": true,
        "parser": "babylon",
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "all",
        "useTabs": false
    },
    "eslintConfig": {
        "env": {
            "node": true,
            "jest": true
        },
        "extends": [
            "airbnb-base",
            "plugin:prettier/recommended"
        ],
        "plugins": [
            "prettier"
        ],
        "root": true,
        "rules": {
            "prettier/prettier": "error",
            "no-param-reassign": [
                "error",
                {
                    "props": false
                }
            ]
        }
    },
    "jest": {
        "verbose": true
    }
}
